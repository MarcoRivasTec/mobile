{
  "name": "@tecma/tecmamovilconnect",
  "version": "1.0.8",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "adev": "set DEV_MODE_ANDROID=1 && node scripts/set-configuration.js",
    "arun": "set DEV_MODE_ANDROID_RUN=1 && node scripts/set-configuration.js",
    "idev": "DEV_MODE_IOS_RUN=1 node scripts/set-configuration.js",
    "pb-a": "set EXPO_PREBUILD_ANDROID=1 && node scripts/set-configuration.js",
    "pb-ac": "set EXPO_PREBUILD_ANDROID_CLEAN=1 && node scripts/set-configuration.js",
    "pb-i": "EXPO_PREBUILD_IOS=1 node scripts/set-configuration.js",
    "pb-ic": "EXPO_PREBUILD_IOS_CLEAN=1 node scripts/set-configuration.js",
    "eas-android": "set EAS_BUILD=1 && node scripts/set-configuration.js",
    "android": "expo run:android",
    "web": "expo start --web",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-clipboard/clipboard": "^1.14.0",
    "@react-native-picker/picker": "2.11.2",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "buffer": "^6.0.3",
    "expo": "^53.0.22",
    "expo-build-properties": "~0.14.8",
    "expo-clipboard": "~7.1.5",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-location": "~18.1.6",
    "expo-secure-store": "~14.2.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "7.2.2",
    "luxon": "^3.5.0",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-bouncy-checkbox": "^4.0.0",
    "react-native-date-picker": "^5.0.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-fast-shadow": "^0.1.1",
    "react-native-file-viewer": "^2.1.5",
    "react-native-flash-message": "^0.4.2",
    "react-native-fs": "^2.20.0",
    "react-native-qrcode-styled": "^0.3.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-signature-canvas": "^5.0.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "react-native-svg-transformer": "^1.3.0"
  },
  "resolutions": {
    "wrap-ansi": "^6.2.0",
    "string-width": "^4.2.0"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ],
    "nohoist": [
      "**/wrap-ansi",
      "**/string-width"
    ]
  },
  "private": true,
  "type": "commonjs",
  "packageManager": "yarn@4.9.4"
}
