{
  "name": "tecmamovil",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "pb-a": "set EXPO_PREBUILD_ANDROID=1 && node scripts/set-reanimated-version.js",
    "pb-ac": "set EXPO_PREBUILD_ANDROID_CLEAN=1 && node scripts/set-reanimated-version.js",
    "pb-i": "EXPO_PREBUILD_IOS=1 node scripts/set-reanimated-version.js",
    "pb-ic": "EXPO_PREBUILD_IOS_CLEAN=1 node scripts/set-reanimated-version.js",
    "eas-android": "set EAS_BUILD=1 && node scripts/set-reanimated-version.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "^1.14.0",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "buffer": "^6.0.3",
    "expo": "51.0.37",
    "expo-app-loading": "^2.1.1",
    "expo-clipboard": "6.0.3",
    "expo-device": "^6.0.2",
    "expo-document-picker": "^12.0.2",
    "expo-file-system": "^17.0.1",
    "expo-font": "^12.0.10",
    "expo-image-picker": "^15.0.7",
    "expo-linear-gradient": "13.0.2",
    "expo-linking": "^6.3.1",
    "expo-sharing": "^12.0.1",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "^1.12.1",
    "lottie-react-native": "6.7.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-bouncy-checkbox": "^4.0.0",
    "react-native-date-picker": "^5.0.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-reanimated": "3.9.0-rc.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "react-native-svg-transformer": "^1.3.0"
  },
  "resolutions": {
    "wrap-ansi": "^6.2.0",
    "string-width": "^4.2.0"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ],
    "nohoist": [
      "**/wrap-ansi",
      "**/string-width"
    ]
  },
  "private": true
}